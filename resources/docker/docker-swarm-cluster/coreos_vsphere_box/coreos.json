{
  "provisioners": [
    {
      "type": "shell",
      "scripts": [
        "coreos.sh"
      ]
    }
  ],
  "builders":[
  {
    "type": "vmware-vmx",
    "source_path": "coreos.vmx",
    "boot_wait": "15s",
    "ssh_username": "vagrant",
    "ssh_password": "vagrant",
    "ssh_wait_timeout": "30s",
    "headless": "true",
    "shutdown_command": "sudo shutdown -P now",
    "vm_name": "coreos"
  }
  ],
  "post-processors": ["vagrant"]
}
